{
    "manifest_version": 3,
    "name": "Cache Cleaner",
    "version": "1.0",
    "description": "Deletes the Cache Storage 'WebResources' before a page is reloaded.",
    "permissions": ["scripting", "tabs", "storage"],
    "host_permissions": [
        "https://duplicates.crm4.dynamics.com/*",
        "https://propad.crm4.dynamics.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Cache Cleaner",
        "default_popup": "popup.html"
    }
}
